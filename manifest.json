{
    "name": "partkeepr",
    "id": "partkeepr",
    "url": "partkeepr.org",
    "packaging_format": 1,
    "description": {
        "en": "Parts inventory management system",
        "fr": "Parts inventory management system"
    },
    "version": "1.0",
    "license": "AGPL-3.0",
    "maintainer": {
        "name": "czen",
        "email": "taccessviolation@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 2.7.2"
    },
    "services": [
        "nginx"
     ],
    "multi_instance": true,
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "ask": {
                    "en": "Choose a domain for partkeepr",
                    "fr": "Choisissez un domaine pour partkeepr"
                },
                "example": "app.domain.org"
            }
        ]
    }
}
